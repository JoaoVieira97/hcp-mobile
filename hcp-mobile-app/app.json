{
  "expo": {
    "name": "H.C.Penafiel",
    "version": "1.1.0",
    "icon": "./assets/logo.png",
    "slug": "HcpMobileApp",
    "privacy": "unlisted",
    "description": "Mobile app for HCPenafiel Team's Management.",
    "sdkVersion": "32.0.0",
    "platforms": [
      "android"
    ],
    "orientation": "portrait",
    "splash": {
      "image": "./assets/hcp_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#dbdbdb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hcp.hcpapp"
    },
    "android": {
      "package": "com.hcp.hcpapp",
      "icon": "./assets/logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/mobile_logo.png",
        "backgroundColor": "#dbdbdb"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "VIBRATE",
        "com.google.android.c2dm.permission.RECEIVE"
      ]
    },
    "notification":{
      "icon": "./assets/logo.png"
    }
  }
}